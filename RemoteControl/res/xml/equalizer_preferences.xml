<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen
       xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:robobunny="http://robobunny.com"
       android:key="equalizer_preferences">
       
        <com.robobunny.SeekBarPreference
        android:key="prefEqualizerRMSThreshold"
        android:title="RMS Threshold"
        android:defaultValue="200"
        android:max="2000"
        robobunny:min="0"
        />
        
        <com.robobunny.SeekBarPreference
        android:key="prefEqualizerFrequencyThreshold"
        android:title="Frequency Threshold"
        android:defaultValue="20"
        android:max="70"
        robobunny:min="0"
        />
    
    <com.inappropirates.remotecontrol.ColorPickerPreference
        android:key="prefEqualizerColor"
        android:title="color"
        />
    
    <CheckBoxPreference
		android:defaultValue="false"
        android:key="prefEqualizerAllLights"
        android:title="all lights" />
    
</PreferenceScreen>